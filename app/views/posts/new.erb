<%= form_for [@post, @location] do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :description %>
<%= f.text_area :description %>
<p>Enter address<%= f.fields_for :location do |location| %></p>
  <%= location.text_field :address %>
  <% end %>
<%= f.file_field :image %>
<%= f.submit %>
<% end %>
