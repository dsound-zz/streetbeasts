<h1>User Posts</h1>

<%= button_to "Create post", new_post_path, method: "get"%>


<% @posts.each do |post| %>

<p>Post title:  <%= link_to post.title, post_path(post) %> </p>
<% if current_user %>
<%= button_to "edit post", edit_post_path(post), method: "get" %><br>
<%= button_to "delete post", post_path(post), method: "delete",
      data: { confirm: "Are you sure?" }%>
  <% end %>

<% end %>
